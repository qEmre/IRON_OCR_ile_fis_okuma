<form method="post" asp-controller="OCR" asp-action="documentSave" enctype="multipart/form-data">
    <label for="imageName">Dosya Adı:</label>
    <input type="text" name="imageName" required />
    <br>

    <label for="file">Resim Seç:</label>
    <input type="file" name="file" />
    <br>

    <button type="submit">Yükle</button>
    <a href="/OCR/documentList" class="btn btn-info" style="width: 175px; margin-left: 1125px; margin-top: 20px;">
        Listeye Git
    </a>
</form>